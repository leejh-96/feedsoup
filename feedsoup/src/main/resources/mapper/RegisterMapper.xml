<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.feed.feedsoup.repository.RegisterRepository">

    <insert id="save" parameterType="RegisterFormDTO">
        INSERT INTO MEMBER (m_type,m_status,m_id,m_password,m_name,m_birthdate,m_nickname,m_phone,m_address,m_date,m_update)
        VALUES('MEMBER','Y',#{register.memberId},#{register.memberPassword},#{register.memberName},#{register.memberBirthdate},
